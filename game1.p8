--[[ enemies and hit cooldown ]]






-- creating the enemy list
	enemies={}

--[[ function that creates
					entries on the enemy list
					for each enemy ]]
function make_enemy(x,y)
	
	return{
		xpos=x,
		ypos=y,
		speed=0.6,
		moving_r=true,
		moving_l=false
	}
	
end

--[[ all the logic of the
					enemies ai ]]
function enemy_ai(e)
	
-- applying coords offset to enemies as well
	local x_offset, y_offset=map_offset()
	
--[[ detecting wether 
					the enemy moves to the
					left or to the right ]]
	if (fget(mget(flr((e.xpos-1)/8)+x_offset,flr((e.ypos+4)/8)+y_offset),0)) then
		
		e.moving_r=true
		e.moving_l=false
		
	elseif (fget(mget(flr((e.xpos+8)/8)+x_offset,flr((e.ypos+4)/8)+y_offset),0)) then
		
		e.moving_r=false
		e.moving_l=true
		
	end
	
--[[ moving the enemy in
					the correct direction ]]
	if (e.moving_r==true) then
		
		e.xpos+=e.speed
		
	elseif (e.moving_l==true) then
		
		e.xpos-=e.speed
		
	end
	
--[[ detecting wether the
					player has collided with
					an enemy or obstacle
					& applying the damage ]]
	if (flr((e.xpos+4)/8)==flr((player.xpos+4)/8) 
	and (flr((e.ypos+4)/8)==flr((player.ypos+4)/8)) 
	and player.can_receive_dmg==true)
	then
		
		player.can_receive_dmg=false
		player.hp-=1
		
	end
	
end

--[[ preventing the player from
					loosing all their hp
					in one hit with a
					cooldown ]]
function hit_cooldown()
	
	if (player.can_receive_dmg==false) then
		
		player.hp_cooldown-=1
		
		if (player.hp_cooldown==0) then
			player.can_receive_dmg=true
			player.hp_cooldown=90
		end
		
	end
	
end
