--[[ physics and logic ]]




--[[ apply the coords offset
					needed when changing
					between lvls for
					collission to work
					properly ]]
function map_offset()
 if (player.onlvl==1) then
  return 0,0
 elseif (player.onlvl==2) then
  return 0,16
 elseif (player.onlvl==3) then
  return 16,16
 end
end

--[[ gather what objects are
					around the players
					character in different
					directions ]]
function tiles_around_player()
	
	local x_offset, y_offset=map_offset()
	
-- getting sprites around player
	obj_down_player=mget(flr((player.xpos+4)/8)+x_offset,flr((player.ypos+8)/8)+y_offset)
 obj_up_player=mget(flr((player.xpos+4)/8)+x_offset,flr((player.ypos)/8)+y_offset)
	obj_r_player=mget(flr((player.xpos+8)/8)+x_offset,flr((player.ypos+4)/8)+y_offset)
 obj_l_player=mget(flr((player.xpos-1)/8)+x_offset,flr((player.ypos+4)/8)+y_offset)
	obj_on_player=mget(flr((player.xpos+4)/8)+x_offset,flr((player.ypos+4)/8)+y_offset)
	obj_foot_player=mget(flr((player.xpos+4)/8)+x_offset,flr((player.ypos+7)/8)+y_offset)
	obj_head_player=mget(flr((player.xpos+4)/8)+x_offset,flr((player.ypos-1)/8)+y_offset)
	
end

--[[ controlling the players
					movement depending on
					their inputs ]]
function players_movement()

--[[ the frictions constant
					used for slowing down
					the player to a stop
					once they stop moving ]]
	local friction=0.3

-- player moving to the right
	if (btn(➡️)) 
	and not (fget(obj_r_player,0)) then
		player.xspeed+=player.xaccel
	end
	
-- player moving to the left
	if (btn(⬅️)) 
	and not (fget(obj_l_player,0)) then
		player.xspeed-=player.xaccel
	end
	
-- player moving up
	if (btn(⬆️) and obj_on_player!=97 
	and player.onair==false) then
		player.yspeed-=3.5 --> jump force
	end

--[[ slowly stop the player if
					theyre not moving anymore ]]
	if not btn(➡️)
	and not btn(⬅️)
	then
		
	-- stop micro movements
		if (player.xspeed<0.1
		and player.xspeed>-0.1) then
			player.xspeed=0
		elseif (player.xspeed>0) then
			player.xspeed-=friction
		elseif (player.xspeed<0) then
			player.xspeed+=friction
		end
		
	end

-- setting a max speed
	if (player.xspeed>=1.5) then
		player.xspeed=1.5
	elseif (player.xspeed<=-1.5) then
		player.xspeed=-1.5
	end

-- applying x movement
	player.xpos+=player.xspeed
	
	tiles_around_player()
	
-- hitting wall on the right
	if player.xspeed>0
	and fget(obj_r_player,0) then
	 player.xspeed=0
	 player.xpos=flr((player.xpos+8)/8)*8-8
	end
	
-- hitting wall on the left
	if player.xspeed<0
	and fget(obj_l_player,0) then
	 player.xspeed=0
	 player.xpos=flr((player.xpos-8)/8)*8+8
	end
	
end

--[[ all the physics
					calculations needed in
					the game ]]
function physics()
	
-- gravitys acceleration
	player.yspeed+=0.3
	
--[[ save in a players attr.
					if their characters in
					the air or not ]]
		if (fget(obj_down_player,0)) then
			player.onair=false
		else
			player.onair=true
		end
	
--[[ check the if the players
					in solid ground, if true,
					disable gravity force and
					push them upwards in case
					they end up inside solid
					sprites. if false, apply
					gravity ]]
	if (player.yspeed>0) then
		if (fget(obj_foot_player,0)) then
			player.yspeed=0
			player.ypos-=1	
			player.onair=false
		elseif (fget(obj_down_player,0)) then
			player.yspeed=0
			player.onair=false
		else
			player.yspeed+=0.3
		end
	end
	
--[[ stop players vertical
					movement if they hit
					solid sprites above them
					and push them downwards
					in case they end up inside
					solid sprites, otherwise
					keep gravitys regular
					logic ]]
	if (player.yspeed<0) then
		if (fget(obj_head_player,0)) then
			player.yspeed=0
			player.ypos+=1
		elseif (fget(obj_up_player,0)) then
			player.yspeed=0
		elseif (player.onair==false) then
			player.yspeed+=0.3
			player.onair=true
		end
	end
		
-- actually applying physics
	player.ypos+=player.yspeed
	
end

--[[ lauch pad feature ]]
function launch_pad()
	
	if (obj_on_player==97) then
		player.yspeed=-4
	end
	
end

--[[ orb jump feature ]]
function orb_jump()
	
	if (obj_on_player==100
	and btn(⬆️)) then
		player.yspeed=-2.8
	end
	
end

--[[ hurting player if they
					touch spikes ]]
function spikes_hurt()
		
		if (obj_on_player==98
	and player.can_receive_dmg==true)
	then
		player.can_receive_dmg=false
		player.hp-=1
	end
	
end

--[[ transport the player from
					one lvl to the next once
					they reach the correct
					coords in their current
					map ]]
function lvl_trans()
	
	if (player.onlvl==1
	and flr((player.xpos+4)/8)==15
	and flr((player.ypos+4)/8)==13)
	then
		
		player.onlvl=2
		player.justchangedlvl=true
		enemies={} --> clean all enemies
	-- adding new enemies
		add(enemies,make_enemy(100,112))
		
	elseif (player.onlvl==2
	and flr((player.xpos+4)/8)==0
	and flr((player.ypos+4)/8)==2)
	then
		
		player.onlvl=3
		player.justchangedlvl=true
		
		enemies={}
		add(enemies,make_enemy(64,112))
		add(enemies,make_enemy(90,112))
	
	elseif (player.onlvl==2
	and flr((player.xpos+4)/8)==0
	and flr((player.ypos+4)/8)==1)
	then
		
		player.onlvl=3
		player.justchangedlvl=true
		
		enemies={}
		add(enemies,make_enemy(64,112))
		add(enemies,make_enemy(90,112))
		
	end
	
end
