--[[ frame rendering ]]




--[[ rendering the player
					sprite in the correct way
					depending on the situation ]]
function player_sprite()
	
-- check if the game has ended
	if (game_over_timer>1) then
	
	--check if theyre hurt
		if (player.can_receive_dmg==true and game_over_timer==45) then
		
		--check their current movement
			if (player.yspeed<0 and btn(➡️)) then
				spr(4,player.xpos,player.ypos)
			elseif (player.yspeed<0 and btn(⬅️)) then
				spr(5,player.xpos,player.ypos)
			elseif (player.yspeed<0) then
				spr(2,player.xpos,player.ypos)
			elseif (player.yspeed>0 and btn(➡️)) then
				spr(6,player.xpos,player.ypos)
			elseif (player.yspeed>0 and btn(⬅️)) then
				spr(7,player.xpos,player.ypos)
			elseif (player.yspeed>0) then
				spr(3,player.xpos,player.ypos)
			elseif (btn(➡️)) then
				spr(8,player.xpos,player.ypos)
			elseif (btn(⬅️)) then
				spr(9,player.xpos,player.ypos)
			else
				spr(1,player.xpos,player.ypos)
			end
		
		elseif (game_over_timer==45) then
		
			if (player.yspeed<0 and btn(➡️)) then
				spr(19,player.xpos,player.ypos)
			elseif (player.yspeed<0 and btn(⬅️)) then
				spr(20,player.xpos,player.ypos)
			elseif (player.yspeed<0) then
				spr(17,player.xpos,player.ypos)
			elseif (player.yspeed>0 and btn(➡️)) then
				spr(21,player.xpos,player.ypos)
			elseif (player.yspeed>0 and btn(⬅️)) then
				spr(22,player.xpos,player.ypos)
			elseif (player.yspeed>0) then
				spr(18,player.xpos,player.ypos)
			elseif (btn(➡️)) then
				spr(23,player.xpos,player.ypos)
			elseif (btn(⬅️)) then
				spr(24,player.xpos,player.ypos)
			else
				spr(16,player.xpos,player.ypos)
			end
		
	-- regular standing sprite
		else
			spr(16,player.xpos,player.ypos)
		end
		
	end
	
end

--[[ rendering the enemies
					sprites depending on their
					movement ]]
function enemy_sprite(e)
	
	if (e.moving_l==true) then
		spr(34,e.xpos,e.ypos)
	elseif (e.moving_r==true) then
		spr(33,e.xpos,e.ypos)
	else
		spr(32,e.xpos,e.ypos)
	end
	
end

--[[ displaying the players
					hp onscreen ]]
function player_hp_display()
	
	if (game_over_timer>1) then
	
		if (player.hp==3) then
			spr(64,16,120)
			spr(65,24,120)
			spr(66,32,120)
		elseif (player.hp==2) then
			spr(64,16,120)
			spr(65,24,120)
			spr(82,32,120)
		elseif (player.hp==1) then
			spr(64,16,120)
			spr(81,24,120)
			spr(82,32,120)
		elseif (player.hp<=0) then
			spr(80,16,120)
			spr(81,24,120)
			spr(82,32,120)
		end

	end
	
end

--[[ show the game over screen,
					cleaning in the process
					the actual screen from
					anything else ]]
function game_over_screen()
	
	cls()
	
	enemies={}
	
	map(16,-1,0,0) --> gameover map
	
end

--[[ show the "you win!" screen
					(same logic as gameover
					screen) ]]
function you_win_screen()
	
	cls()
	
	enemies={}
	
	map(32,-1,0,0)
	
end

--[[ drawing the actual map
					according to what lvl the
					player is at ]]
function draw_level()
	
	if (player.onlvl==1) then
		map(0,0,0,0)
	elseif (player.onlvl==2) then
		if (player.justchangedlvl==true) then
			player.xpos=8
   player.ypos=104
   player.justchangedlvl=false
		end
		map(0,16,0,0)
	elseif (player.onlvl==3) then
		if (player.justchangedlvl==true) then
			player.xpos=8
			player.ypos=112
			player.justchangedlvl=false
		end
		map(16,16,0,0)
	end
	
end
