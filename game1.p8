--[[ main 3 functions ]]



function _init()

--[[ we create the player with
					all their attributes ]]
	player={
		xpos=16,
		ypos=24,
		xspeed=0,
		xaccel=0.3,
		xmaxspeed=1.5,
		yspeed=0,
		onair=false,
		hp=3,
		hp_cooldown=90,
		can_receive_dmg=true,
		onlvl=1,
		justchangedlvl=false
	}

-- set gameover timer
	game_over_timer=45
	
-- first lvl enemies
	add(enemies,make_enemy(48,32))
	add(enemies,make_enemy(72,72))
	add(enemies,make_enemy(64,112))
	
end

--[[ logic processed before
					every frame ]]
function _update()

--[[ run the logic if the
					gameover timer hasnt
					started ticking ]]
	if (game_over_timer==45) then
		lvl_trans()
	
		tiles_around_player()
		
		physics()
		
		launch_pad()
		
		orb_jump()
		
		players_movement()
		
		spikes_hurt()
		
--[[ we apply the ai logic to 
					every enemy present
					in the enemies list]]
		for e in all(enemies) do
			enemy_ai(e)
		end
		
		hit_cooldown()
	end
	
--[[ if the players hp drops
					to zero, we start the
					gameover timer ]]
	if (player.hp==0) then
		game_over_timer-=1
			if(game_over_timer==0) then
				stop() --> close the game
			end
	end
	
--[[ players terminal
					velocity in the -y axis ]]
	if (player.yspeed>=6) then
		player.yspeed=6
	end
	
end

--[[ rendering of every frame ]]
function _draw()

	cls()
	
	draw_level()
	
--[[ if the player reaches the
					object located at the
					end of the game, show them
					the "you win!" screen &
					stop the game logic &
					rendering ]]
	if (obj_on_player==113) then
		game_over_timer=0
		you_win_screen()
		
--[[ if the gameover timer
					reaches 0 before they win,
					show them the gameover
					screen ]]
	elseif (game_over_timer==1) then
		game_over_screen()
	end

--[[ render an enemy sprite
					for every entry in the
					enemy list ]]
	for e in all(enemies) do
		enemy_sprite(e)
	end
	
	player_hp_display()
	
	player_sprite()
	
end
